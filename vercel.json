{
  "rewrites": [
    {
      "source": "/introducers",
      "destination": "/introducers.html"
    },
    {
      "source": "/glass",
      "destination": "https://glass-human-diagnostic.vercel.app"
    },
    {
      "source": "/glass/(.*)",
      "destination": "https://glass-human-diagnostic.vercel.app/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/process-payouts",
      "schedule": "0 2 5 * *"
    }
  ]
}
